<h1 class="togo">To Go</h1>

<div class="product-display">{{#each toGo}}


<div class="card" style="width: 18rem;">
  <img class="card-img-top" src="{{imageUrl}}" alt="Card image cap">
  <div class="card-body">
    <h5 class="card-title"><a href="/products/{{_id}}">{{name}}</a></h5>
    <p class="card-text">{{description}}</p>
    <a href="/favourites" class="btn btn-primary">Favorite</a>
  </div>
</div>


{{/each}}
</div>


